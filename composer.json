    "require": {
        "php": ">=5.6",
        "doctrine/cache": "~1.6.2",
        "guzzlehttp/guzzle": "^6.5",
        "ext-fileinfo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Imghub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imghub\\Tests\\": "src/tests"
        }
    },
    "authors": [
        {
            "name": "thatfixer",
